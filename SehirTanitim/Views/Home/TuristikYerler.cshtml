@using System.Linq;

@model List<SehirTanitim.Models.TouristPlace>
<h2>Turistik Yerler</h2>
<ul>
    @foreach (var place in Model)
    {
        <li>
            <strong>@place.Name</strong> - @place.Description <br />
            <img src="@place.ImageUrl" style="max-width:400px;" />
        </li>
    }
</ul>
